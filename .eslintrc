{
    "extends": ["react-app", "react-app/jest", "prettier", "prettier/prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["warn", {
            "singleQuote": true,
            "endOfLine": "auto"
            }
        ]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint"
}